{
  "name": "@ebay/pipeline-metrics-ebay",
  "version": "1.0.1",
  "description": "Duration tracking for Node.js",
  "main": "index.js",
  "scripts": {
    "test-app": "NODE_ENV=test DEPLOY_ENV=test mocha test --timeout 20s --exit",
    "test-spec": "NODE_ENV=test DEPLOY_ENV=test mocha src/**/*.spec.js --timeout 20s --exit",
    "test-types": "mocha builders/** --recursive --timeout 20s --exit",
    "test": "npm run test-spec && npm run test-types && npm run test-app",
    "lint": "eslint --quiet -c .eslintrc .",
    "lint-fix": "eslint --quiet -c .eslintrc . --fix",
    "coverage": "rm -rf .beans .cache & NODE_ENV=test DEPLOY_ENV=test nyc --reporter=html --reporter=text mocha test/*.js --- --timeout 15s"
  },
  "repository": {
    "type": "git",
    "url": "git@github.corp.ebay.com:nodejs/pipeline-metrics-ebay.git"
  },
  "author": "kabhatiya@ebay.com",
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  },
  "dependencies": {
    "@ebay/logging-inc": "^4.0.5",
    "@ebay/prometheus-ebay": "^3.0.6",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "mocha": "^6.0.2",
    "supertest": "^4",
    "nyc": "^15.1.0",
    "@ebay/monitor-inc": "^5.1.0",
    "eslint": "^5.10.0",
    "eslint-config-ebay": "^1.0.0",
    "eslint-plugin-mocha": "^5.2.0"
  },
  "license": "ISC"
}
