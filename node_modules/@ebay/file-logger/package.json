{
  "name": "@ebay/file-logger",
  "version": "1.0.2",
  "description": "File logger",
  "main": "index.js",
  "scripts": {
    "test": "rm -rf .beans .cache logs && NODE_ENV=test DEPLOY_ENV=test mocha --reporter spec --timeout 10s",
    "lint": "eslint . -f checkstyle -c .eslintrc",
    "lint-fix": "eslint --quiet -c .eslintrc . --fix",
    "coverage": "rm -rf .beans .cache logs & NODE_ENV=test DEPLOY_ENV=test nyc --reporter=html --reporter=text mocha test/*.js --- --timeout 15s"
  },
  "repository": {
    "type": "git",
    "url": "git@github.corp.ebay.com:nodejs/file-logger.git"
  },
  "keywords": [
    "file",
    "logger"
  ],
  "author": "mdathrika@ebay.com",
  "license": "ISC",
  "dependencies": {
    "pino": "^6.11.3",
    "@ebay/environment-ebay": "^1.0.4"
  },
  "devDependencies": {
    "mocha": "^3",
    "shelljs": "^0.3",
    "eslint": "^5.0.0",
    "eslint-config-ebay": "^1.0.0",
    "eslint-plugin-mocha": "^5.0.0"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  }
}
