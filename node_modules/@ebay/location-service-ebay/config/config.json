{
    "services": {
        "locationservice": {
            "urn": "urn:REST:lbservice",
            "rejectUnauthorized": false,
            "socketTimeout": 2000,
            "protocol": "https:",
            "hostname": "lbservice.vip.ebay.com",
            "basepath": "/base/location/v3",
            "transport": "generic",
            "method": "GET",            
            "instruments": ["oauth","error-handler", "retry", "config-bean", "cal"],
            "oauth": {
                "scope": "https://api.ebay.com/oauth/scope/base@application",
                "token": ["app"]
            }
        }
    }
}
