{
    "service-ebay": {
        "consumer-status": {
            "enabled": true
        }
    },

    "express": {
        "env": "",
        "x-powered-by": false,
        "trust proxy": false,
        "jsonp callback name": null,
        "json replacer": null,
        "json spaces": 0,
        "case sensitive routing": false,
        "strict routing": false,
        "views": null,
        "view cache": false,
        "view engine": false
    },

    "api": {
        "security-providers": {
            "app_scope_auth": "require:@ebay/service-security-ebay"
        }
    },

    "view engines": false,

    "middleware": {
        "shutdown": {
            "enabled": true,
            "priority": 0,
            "phase": "preRoute",
            "module": {
                "name": "@ebay/service-ebay/middleware/shutdown",
                "arguments": [{
                    "timeout": 30000,
                    "template": null
                }]
            }
        },

        "gatekeeper": {
            "enabled": true,
            "phase": "preRoute",
            "priority": 1,
            "module": {
                "name": "@ebay/gatekeeper-ebay",
                "arguments": [{
                    "errorHandler": "path:../lib/error-handler"
                }]
            }
        },

        "starttime": {
          "enabled": true,
          "priority": 2,
          "phase": "preRoute",
          "module": {
              "name": "@ebay/pipeline-metrics-ebay/middleware/starttime"
          }
        },

        "opentelemetry-ebay": {
            "enabled": true,
            "priority": 3,
            "phase": "preRoute",
            "module": {
                "name": "@ebay/opentelemetry-ebay/middleware"
            }
        },

        "sanitize": {
            "enabled": true,
            "phase": "preRoute",
            "priority": 6,
            "module": "path:../middleware/sanitize-headers"
        },

        "static": {
            "enabled": true,
            "route": "/",
            "phase": "preRoute",
            "priority": 20,
            "module": {
                "name": "serve-static",
                "arguments": [ "path:../public/swagger-ui/dist" ]
            }
        },

        "api_docs": {
            "enabled": true,
            "route": "/api-docs",
            "priority": 25,
            "phase": "preRoute",
            "module": {
                "name": "path:../middleware/api-docs"
            }
        },

        "logger": {
            "enabled": false,
            "phase": "preRoute",
            "priority": 30,
            "module": "morgan"
        },

        "requestlocal": {
            "enabled": true,
            "priority": 40,
            "phase": "preRoute",
            "module": {
                "name": "request-local/middleware",
                "method": "create"
            }
        },

        "cal": {
            "enabled": true,
            "priority": 50,
            "phase": "preRoute",
            "module": {
                "name": "@ebay/cal/middleware",
                "arguments": [{
                    "enableNestedCal": true,
                    "bufferFlushDelay": 500
                }]
            }
        },

        "consumer-status": {
            "enabled": true,
            "priority": 55,
            "phase": "preRoute",
            "module": {
                "name": "path:../middleware/consumer-status"
            }
        },

        "json": {
            "enabled": true,
            "priority": 60,
            "phase": "preRoute",
            "module": {
                "name": "body-parser",
                "method": "json"
            }
        },

        "urlencoded": {
            "enabled": true,
            "priority": 60,
            "phase": "preRoute",
            "module": {
                "name": "body-parser",
                "method": "urlencoded",
                "arguments": [{ "extended": true }]
            }
        },

        "multipart": {
            "enabled": true,
            "priority": 61,
            "phase": "preRoute",
            "module": {
                "name": "path:../middleware/multipart"
            }
        },

        "route_config": {
            "enabled": true,
            "priority": 55,
            "phase": "route",
            "module": {
                "name": "path:../middleware/router"
            }
        },

        "commonsinc": {
            "enabled": true,
            "priority": 60,
            "phase": "route",
            "module": {
                "name": "@ebay/commons-inc/middleware",
                "arguments": [{
                    "cal": {
                        "type": "cal",
                        "formatter": "cal",
                        "settings": {
                            "host": "config:commons.cal.host",
                            "port": "config:commons.cal.port",
                            "environment": "config:commons.cal.environment"
                        }
                    }
                }]
            }
        },

        "commonsebay": {
            "enabled": true,
            "priority": 80,
            "phase": "route",
            "module": {
                "name": "@ebay/commons-ebay/middleware",
                "arguments": [{
                    "type": "service"
                }]
            }
        },

        "trustfabric": {
            "enabled": true,
            "priority": 90,
            "phase": "route",
            "module": "@ebay/trust-fabric-ebay/middleware"
        },

        "tracking": {
            "enabled": true,
            "priority": 91,
            "phase": "route",
            "module": "@ebay/tracking-ebay/middleware"
        },

        "oauth": {
            "enabled": true,
            "priority": 92,
            "phase": "route",
            "module": "@ebay/oauth-ebay"
        },

        "locale": {
            "enabled": true,
            "priority": 93,
            "phase": "route",
            "module": "@ebay/locale-ebay"
        },

        "monitor": {
             "enabled": true,
             "priority": 100,
             "phase": "route",
             "module": {
                 "name": "@ebay/monitor-inc",
                 "method": "init"
             }
        },

        "prometheus-ebay": {
            "enabled": true,
            "priority": 101,
            "phase": "route",
            "module": {
                "name": "@ebay/prometheus-ebay/middleware"
            }
        },

        "rci": {
            "enabled": true,
            "priority": 200,
            "phase": "route",
            "module": {
                "name": "path:../middleware/rci"
            }
        },

        "stoptime": {
            "enabled": true,
            "priority": 450,
            "phase": "route",
            "module": {
                "name": "@ebay/pipeline-metrics-ebay/middleware/stoptime"
            }
        },

        "controller": {
            "enabled": true,
            "phase": "route",
            "priority": 500
        },

        "errors": {
            "enabled": true,
            "priority": 1000,
            "phase": "postRoute",
            "module": "@ebay/service-errors-ebay"
        }

    },

    "commons": {
        "cal": {
            "host": "cal.vip",
            "port": 1118,
            "environment": "prod"
        }
    },

    "@ConfigBean": {
        "service-ebay": {
            "common": [
                "service-ebay"
            ]
        }
    }
}
