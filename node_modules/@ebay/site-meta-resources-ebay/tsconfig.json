{
    "compilerOptions": {
        "target": "es2022",
        "module": "commonjs",
        "sourceMap": true,
        "declaration": true,
        "stripInternal": true,
        "importHelpers": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist"
    },
    "include": ["gen"]
}