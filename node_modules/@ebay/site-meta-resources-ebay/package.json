{
    "name": "@ebay/site-meta-resources-ebay",
    "version": "0.1.28",
    "description": "he module contains site metadata only and is decoupled from @ebay/site-meta-ebay module to allow frequent updates without the need to publish code modules",
    "keywords": [
        "metadata",
        "@ebay/site-meta-ebay"
    ],
    "author": "dsemenov@ebay.com",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "eBay",
    "repository": {
        "type": "git",
        "url": "git://github.corp.ebay.com/nodejs/site-meta-resources-ebay.git"
    },
    "publishConfig": {
        "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
    },
    "scripts": {
        "clean": "rm -rf dist gen",
        "build": "npm run clean && node scripts/generate.js && tsc",
        "test": "node ./scripts/validate-ebaysites && node ./scripts/validate-localized-lists && npm run test:types",
        "coverage": "echo nothing to cover",
        "lint": "tsc --noEmit && tslint --fix -t codeFrame -c tslint.json '{src,test}/**/*.ts'",
        "prepublishOnly": "npm run test",
        "test:types": "npm run build && mocha ./test/*.ts -r ts-node/register"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.11.17",
        "ajv": "^8.11.0",
        "dtslint": "^4.2.1",
        "mocha": "^10.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
