{
    "experimentation": {
        "enabled": true,
        "a-pool": {
            "enabled": false,
            "connectTimeout": 400,
            "socketTimeout": 2000,
            "botAllowed": false
        },
        "b-pool": {
            "enabled": false
        },
        "useSession": false,
        "sessionLifetime": 300000, //5 min
        "deprecation": {
            "enabled": true,
            "errorStartDate": 1712073600000 // Apr 02, 2024 09:00 AM GMT -7 
            
        }
    },

    "services": {
        "experimentation": {
            "urn": "urn:REST:epsvc",
            "socketTimeout": 200,
            "protocol": "https:",
            "hostname": "epsvc.vip.ebay.com",
            "basepath": "/epexesvc/ExperimentationExecutionService/v4/getQualifiedTreatments",
            "transport": "generic",
            "method": "POST",
            "force-retry": true,
            "https": {
                "keepAlive": true,
                "keepAliveMsecs": 20000
            },
            "headers": {
                "content-type": "application/json",
                "accept": "application/json"
            },
            "instruments": ["outbound-context", "error-handler", "retry", "config-bean", "cal"]
        }
    },

    "@ConfigBean": {
        "experimentation-ebay": {
            "common": [
                "experimentation"
            ]
        }
    }
}