{
  "name": "@ebay/telemetry-ebay",
  "version": "1.0.18",
  "description": "Build & Startup Telemetry",
  "main": "index.js",
  "bin": {
    "collect": "./bin/collect"
  },
  "scripts": {
    "lint": "eslint . --fix -f checkstyle",
    "clean": "rm -rf .beans .cache ",
    "test": "yarn run clean && NODE_ENV=test DEPLOY_ENV=test mocha --sort --reporter spec --timeout 10000  test/*.js",
    "coverage": "rm -rf .beans .cache && NODE_ENV=test DEPLOY_ENV=test nyc --report-dir gh-pages/coverage --reporter=cobertura --reporter=html --reporter=lcov --reporter=text mocha --sort --reporter spec --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.corp.ebay.com:nodejs/telemetry-ebay.git"
  },
  "keywords": [
    "telemetry",
    "build",
    "startup"
  ],
  "author": "mdathrika@ebay.com",
  "dependencies": {
    "debug": "^4.3.1",
    "ip": "^1.1.5",
    "@ebay/httpunch": "^1"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-ebay": "^1.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "nyc": "^13.1.0",
    "request": "^2.88.0"
  },
  "license": "ISC",
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  }
}
