{
    "services": {
        "ebay-fetch-test-categoryService": {
            "protocol": "http:",
            "hostname": "catsvc.qa.ebay.com",
            "basepath": "/catalog/v1/category_tree/0/category/6001",
            "method": "GET",
            "socketTimeout": 5000,
            "headers": {
                "Accept": "application/json",
                "Content-Type": "application/json"
            },
            "oauth": {
                "token": [
                    "app"
                ],
                "scope": [
                    "https://api.ebay.com/oauth/scope/core@application"
                ]
            },
            "instruments": [
                "error-handler"
            ]
        }
    }
}