{
  "name": "@ebay/ebay-fetch",
  "version": "4.0.2",
  "description": "Fetch-like module for connecting to downstream services",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.corp.ebay.com:Wonderland/ebay-fetch.git"
  },
  "keywords": [
    "service",
    "nodejs"
  ],
  "author": "Qing Chen <qchen7@ebay.com>",
  "contributors": [
    "Andrew Chalkley <achalkley@ebay.com>",
    "Shobith Alva <salva@ebay.com>",
    "Jonathan Calvin <jocalvin@ebay.com>",
    "Qing Chen <qchen7@ebay.com>"
  ],
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-wonderland/"
  },
  "scripts": {
    "clean": "rm -rf .nyc_output coverage",
    "coverage": "npm run clean && nyc npm run test",
    "lint": "eslint . -o .eslint-checkstyle.xml -f checkstyle -c .eslintrc",
    "lint-dev": "eslint .",
    "lintfix": "eslint . --fix",
    "show-coverage": "open coverage/nyc/lcov-report/index.html",
    "test": "NODE_ENV=test DEPLOY_ENV=test mocha ./test --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi-reporters.json --recursive --timeout 10s --exit"
  },
  "dependencies": {
    "@ebay/utils": "^2.0.2",
    "nock": "^13.0.11",
    "@ebay/service-ebay": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "chai": "^4.3.4",
    "eslint": "^7",
    "eslint-config-ebay": "^1.0.0",
    "eslint-plugin-mocha": "^9",
    "mocha": "^9",
    "mocha-junit-reporter": "^2",
    "mocha-multi-reporters": "^1.1.7",
    "node-fetch": "^2.6.1",
    "nyc": "^15"
  },
  "pre-commit": [
    "lint-dev",
    "test"
  ],
  "engines": {
    "node": ">=16"
  },
  "types": "types/index.d.ts"
}
