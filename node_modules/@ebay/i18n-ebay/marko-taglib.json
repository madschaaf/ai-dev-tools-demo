{
    "tags": {
        "_i18n-use-root": {
            "renderer": "./marko/i18n-use-root/renderer",
            "attributes": {
                "bundleNames": "string"
            },
            "import-var": {
                "dirname": "__dirname"
            },
            "vars": []
        },
        "i18n-use": {
            "node-class": "./marko/i18n-use/node",
            "attributes": {
                "*": "string"
            },
            "import-var": {
                "dirname": "__dirname"
            },
            "vars": [
                "out"
            ],
            "transformer": {
                "path": "./marko/transformer"
            }
        },
        "i18n-bundle": {
            "node-class": "./marko/i18n-bundle/node",
            "attributes": {
                "path": "string",
                "var": "string"
            },
            "import-var": {
                "dirname": "__dirname"
            },
            "transformer": {
                "path": "./marko/transformer"
            }
        }
    }
}