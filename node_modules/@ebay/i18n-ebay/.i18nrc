{

    "validate": [
        "tag",
        "name",
        "placeholder"
    ],

    "placeholders": {
        "money": {
            "$options": ["currencySymbolType", "emitInternalDecimal"],
            "currencySymbolType": [
                "STANDARD",
                "SIMPLE",
                "FANCY",
                "ALL"
            ],
            "emitInternalDecimal": [
                true,
                false
            ]
        },
        "date": {
            "$options": ["dateFormat", "timeFormat"],
            "dateFormat": [
                "NONE",
                "SHORT",
                "MEDIUM",
                "NUMERIC",
                "NUMERIC_NO_DAY",
                "LONG",
                "FULL",
                "FULL_NO_YEAR",
                "FULL_COMPLETE"
            ],
            "timeFormat": [
                "NONE",
                "SHORT",
                "SHORT_ZONE",
                "MEDIUM",
                "LONG",
                "LONG_AMPM"
            ]
        },
        "link": {
            "secure": [
                true,
                false
            ]
        },
        "number": {}
    },

    "tags": {
