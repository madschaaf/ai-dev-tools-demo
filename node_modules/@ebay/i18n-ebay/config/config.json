{
    "i18n-ebay": {
        "onlySiteSupportLanguages": true,
        "default-locale": "en",
        "mini-sites": {
            "de-AT": "de-DE",
            "de-CH": "de-DE",
            "fr-BE": "fr-FR",
            "en-IE": "en-GB",
            "en-MY": "en-SG",
            "en-PH": "en-SG"
        },
        "preload": [
        ],
        "formatters": {
            "ph": "path:../formatters/ph",
            "tag:DPH": "path:../formatters/dph",
            "ph:link": "path:../formatters/link",
            "tag:a": "path:../formatters/a",
            "ph:date": "path:../formatters/date",
            "ph:money": "path:../formatters/money",
            "ph:number": "path:../formatters/number",
            "ph:string": "path:../formatters/string",
            "simple": "path:../formatters/simple",
            "complex": "path:../formatters/complex",
            "tag": "path:../formatters/tag"
        }
    },

    "@ConfigBean": {
        "i18n-ebay": [
            "i18n-ebay:onlySiteSupportLanguages"
        ]
    }
}
