{
    "name": "@ebay/i18n-ebay",
    "version": "5.11.2",
    "description": "i18n support for eBay",
    "author": "rragan",
    "repository": "git@github.corp.ebay.com:nodejs/i18n-ebay.git",
    "main": "./lib/index.js",
    "browser": "./lib/index-browser.js",
    "types": "types/index.d.ts",
    "scripts": {
        "escheck": "es-check es5 dist/*.js formatters/complex.js formatters/dph.js formatters/ph.js formatters/simple.js formatters/tag.js lib/ResolvedContent.js lib/ContentBundle.js lib/utils.js",
        "lint": "make lint",
        "test:types": "dtslint types",
        "test": "yarn prepublishOnly && make test",
        "testd": "LASSO_VERSION=3 make test",
        "test-debug": "make test-debug",
        "coverage": "nyc --dir gh-pages/coverage yarn test",
        "build": "babel parser.js -d dist",
        "prepublishOnly": "npm run build && npm run escheck"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "version-keeper": {
        "ignore": [
            "spud",
            "marko"
        ]
    },
    "dependencies": {
        "@ebay/common-types-ebay": "^0",
        "@ebay/locale-formatter-ebay": "^1",
        "@ebay/logging-inc": "^4",
        "@ebay/marko-context-ebay": "^1.0.1",
        "@ebay/module-config-inc": "^4",
        "@ebay/urls-ebay": "^4.0.0",
        "@ebay/utils-ebay": "^1",
        "@nodelib/fs.walk": "3.0.1",
        "async": "^3",
        "mixin-deep": "^2",
        "objutil": "^0",
        "raptor-async": "^1",
        "raptor-util": "^1",
        "spud": "^0.0.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.24.5",
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "@ebay/commons-ebay": "^5.3.0",
        "@ebay/cookies-ebay": "^4",
        "@ebay/locale-ebay": "^4.4.1",
        "@ebay/monitor-inc": "^5.1.0",
        "@ebay/validate-internals-ebay": "^5",
        "chai": "^4",
        "dtslint": "^4.2.1",
        "es-check": "5.0.0",
        "express": "^4",
        "jshint": "^2",
        "lasso": "^3",
        "mocha": "^10",
        "mocha-cobertura-reporter": "^1",
        "nyc": "^15.1.0",
        "optimizer": "^1",
        "raptor-modules": "^1",
        "request": "^2",
        "request-local": "^1.0.5",
        "resolve-from": "^5",
        "shelljs": "^0.8.5",
        "supertest": "^6",
        "typescript": "^4.9.4",
        "uuid": "^9",
        "vitest": "3.0.4"
    },
    "publishConfig": {
        "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
    }
}
