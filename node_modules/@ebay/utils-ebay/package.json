  "description": "Small collection of Utility / Helper Methods attributed to Request , Response Flow",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test:type": "tsc ./types/index.d.ts --noEmit",
    "test": "yarn test:type && NODE_ENV=test DEPLOY_ENV=test mocha test/*.js  --timeout 15s",
    "lint": "./node_modules/.bin/jshint lib test --config ./.jshintrc",
    "coverage": "NODE_ENV=test DEPLOY_ENV=test istanbul cover --dir gh-pages/coverage ./node_modules/.bin/_mocha test/*.js --- --timeout 15s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.corp.ebay.com/nodejs/utils-ebay.git"
  },
  "keywords": [
    "Small",
    "collection",
    "of",
    "Utility",
    "/",
    "Helper",
    "Methods",
    "attributed",
    "to",
    "Request",
    "Response",
    "Flow"
  ],
  "author": "RameshRM <ramahadevan@ebay.com>",
  "contributors": [
    {
      "name": "Mahesh Dathrika",
      "email": "mdathrika@ebay.com"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "istanbul": "^0.4.5",
    "jshint": "^2",
    "mocha": "^8",
    "sinon": "^15.0.4",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  },
  "dependencies": {
    "@ebay/app-context-ebay": "^1.1.2",
    "ip": "^1"
  }
