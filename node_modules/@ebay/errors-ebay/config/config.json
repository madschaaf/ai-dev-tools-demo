{
    "errors-ebay": {
        "email": {
            "enabled": true,
            "maxCount": 25,
            "sendDeployEmail": true,
            "sendOomEmail": true,
            "sendUncaughtEmail": true
        }
    },

      "@ConfigBean": {
          "errors-ebay": {
            "common": [
              "errors-ebay:email"
            ]
          }
      },

      "@RaptorConfig": {
          "errors-ebay": [
              {
                  "target": "Global",
                  "project": "ebaykrakn",
                  "config": "EmailConfig",
                  "version": "1.0.0"
              }
          ]
      }
}
