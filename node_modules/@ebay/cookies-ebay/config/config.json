{
    "cookies-ebay": {
        "environmentBasedCookieDomainEnabled": false,
        "cookieDomainValidatorEnabled": true,
        "secureCookieEnabled": false,
        "phase1SecureCookies": [
            "dp1",
            "nonsession",
            "s",
            "sid"
        ],
        "secureCookiePhase2Enabled": false,
        "blockUnmanagedCookies": false,
        "unmanagedCookiesAllowList": [],
        "blackListedCookies": [],
        "httpOnlyCookies": [],
        "refreshInterval": 900000
    },

    "services": {
        "sessionkeysvc": {
            "urn": "urn:REST:seskeysvc",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "seskeysvc.vip.ebay.com",
            "basepath": "/seskeysvc/SessionKeyResource/v2",
            "transport": "generic",
            "socketTimeout": 5000,
            "retry": 1,
            "method": "GET",
            "instruments": ["app-token", "error-handler", "retry", "config-bean", "cal"],
            "headers": {
                "Content-Type": "application/json",
                "Accept": "application/json"
            },
            "error-handler" : {
                "success-codes": [404]
            }
        },
        "authzCookies": {
            "urn": "urn:REST:authzsvc",
            "rejectUnauthorized": false,
            "socketTimeout": 3000,
            "maxKeepAliveTime": 5000,
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "authzsvc-tls.vip.ebay.com",
            "basepath": "/authzsvc/api/v1/cookiecfg",
            "transport": "generic",
            "method": "GET",
            "instruments": ["https", "error-handler", "retry", "config-bean", "cal"]
        }
    },

    "@ConfigBean": {
        "cookies-ebay": {
            "common": [
                "cookies-ebay"
            ]
        }
    }
}
