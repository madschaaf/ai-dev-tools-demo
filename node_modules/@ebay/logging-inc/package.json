{
  "name": "@ebay/logging-inc",
  "version": "4.1.3",
  "description": "Logging with transaction support for paypal/ebay app",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test:types": "dtslint types",
    "test:spec": "rm -rf .beans .cache && NODE_ENV=test DEBUG=dlogger* mocha --require chai --reporter spec --timeout 15s test/*.js",
    "test": "yarn test:types && yarn test:spec",
    "lint": "jshint lib test --config .jshintrc",
    "coverage": "nyc --dir gh-pages/coverage yarn test:spec"
  },
  "repository": {
    "type": "git",
    "url": "http://github.paypal.com/nodejs/logging-inc.git"
  },
  "keywords": [
    "logging",
    "@ebay/cal",
    "logger"
  ],
  "author": "Dmytro Semenov <dsemenov@ebay.com>",
  "license": "BSD",
  "dependencies": {
    "@ebay/cal": "^4",
    "@ebay/module-config-inc": "^4",
    "objutil": "^0.4"
  },
  "devDependencies": {
    "@ebay/cal-publishing-inc": "^4",
    "@types/node": "^18.7.1",
    "chai": "^3",
    "dtslint": "^4.2.1",
    "istanbul": "^0.4.5",
    "jshint": "^2",
    "mocha": "^3",
    "nyc": "^15.1.0",
    "shelljs": "^0.8.4",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs-scoped/"
  },
  "resolutions": {
    "**/@definitelytyped/typescript-versions": "0.0.121",
    "**/@definitelytyped/header-parser": "0.0.121",
    "**/@definitelytyped/utils": "0.0.121"
  }
}
