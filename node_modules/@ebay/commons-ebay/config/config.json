{
    "commons": {
        "cal": {
            "host": "cal.vip",
            "port": 1118,
            "environment": "prod",
            "IntlDiffCmdName": false,
            "CountryDiffCmdName": false
        }
    },
    "services": {
        "currency-list-svc": {
            "urn": "urn:REST:seskeysvc",
            "dnslookup": false,
            "socketTimeout": 2000,
            "protocol": "https:",
            "hostname": "seskeysvc.vip.ebay.com",
            "basepath": "/seskeysvc/kernelsvc/v1/currency-list",
            "transport": "generic",
            "method": "GET",
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    },

    "@ConfigBean": {
        "commons-ebay": {
            "general": [
                "commons:cal"
            ]
        }
    }
}
