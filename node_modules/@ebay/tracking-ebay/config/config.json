{
    "tracking-ebay": {
        "webview": {
            "enable": true
        },
        "pulsarMetadata": true,
        "hmacDmsSvc": true
    },

    "services": {
        "ebaytrackingservice": {
            "urn": "urn:SOA:EbayTrackingServiceV1",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "taas.vip.ebay.com",
            "basepath": "_soa_/services/tracking/EbayTrackingServiceV1/v1/",
            "transport": "generic",
            "method": "POST",
            "socketTimeout": 200,
            "retry": 1,
            "force-retry": true,
            "headers": {
                "X-EBAY-SOA-SERVICE-VERSION": "1.0.0"
            },
            "wsdl": "path:./ebay-tracking-service.wsdl",
            "namespaces": {
                "enable": true,
                "list": {
                    "mandatoryParams": { "byParent": true },
                    "otherParams": { "byParent": true },
                    "cookieData": { "byParent": true },
                    "sojData": { "byParent": true },
                    "requestParam": { "byParent": true }
                }
            },
            "service": "EbayTrackingService",
            "instruments": ["soap", "error-handler", "retry", "config-bean", "cal"]
        },
        "tracking20service": {
            "urn": "urn:REST:trk20svc",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "trk20svc.vip.ebay.com",
            "basepath": "/trk20svc/TrackingResource/v1",
            "transport": "generic",
            "method": "POST",
            "retry": 1,
            "socketTimeout": 5000,
            "connectTimeout": 1000,
            "headers": {
                "Content-Type": "application/json"
            },
            "instruments": ["error-handler", "retry", "config-bean", "cal", "tracking", "outbound-context"]
        },
        "tracking20metadataservice": {
            "urn": "urn:REST:pmsvc",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "pmsvc.vip.ebay.com",
            "basepath": "/pmsvc",
            "transport": "generic",
            "method": "GET",
            "retry": 0,
            "socketTimeout": 5000,
            "connectTimeout": 1000,
            "headers": {
                "Content-Type": "application/json",
                "Accept":"application/json"
            },
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        },
        "hmacDmsSvc": {
            "urn": "urn:REST:dmssvc",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "dmssvc.vip.ebay.com",
            "basepath": "/identity/v3/device/application/guid/validate",
            "transport": "generic",
            "method": "POST",
            "retry": 0,
            "socketTimeout": 5000,
            "connectTimeout": 1000,
            "headers": {
                "Content-Type": "application/json",
                "Accept-Language": "en-US",
                "Accept":"application/json"
            },
            "instruments": ["error-handler", "retry", "config-bean", "cal", "app-token"]
        }
    },

    "@ConfigBean": {
        "tracking-ebay": {
            "common": [
                "tracking-ebay"
            ]
        }
    }
}
