{
  "name": "@ebay/hostip-checker-ebay",
  "version": "4.1.2",
  "description": "IP/Host checker utility that helps figure out whether or not a host belongs to eBay's network topology",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js",
    "test:types": "dtslint types",
    "test:spec": "rm -rf .beans .cache && NODE_ENV=test DEPLOY_ENV=test mocha test/*.js  --timeout 15s",
    "test": "npm run test:types && npm run test:spec",
    "lint": "./node_modules/.bin/jshint ./lib --config ./.jshintrc",
    "coverage": "NODE_ENV=test DEPLOY_ENV=test istanbul cover --dir gh-pages/coverage ./node_modules/.bin/_mocha test/*.js --- --timeout 15s"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.corp.ebay.com/nodejs/hostip-checker-ebay.git"
  },
  "keywords": [
    "ip",
    "host",
    "internal",
    "domain",
    "network",
    "utility"
  ],
  "author": "Chothendavida, Subeesh <suchothendav@paypal.com>",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@ebay/cal": "^4",
    "@ebay/monitor-inc": "^5",
    "@ebay/validate-internals-ebay": "^5",
    "chai": "^3.5.0",
    "dtslint": "^4.2.1",
    "istanbul": "^0",
    "jshint": "^2",
    "mocha": "^3.5.3",
    "nock": "^13",
    "shelljs": "^0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@ebay/environment-ebay": "^1",
    "@ebay/logging-inc": "^4.0.5",
    "@ebay/module-config-inc": "^4",
    "@ebay/service-client-ebay": "^4",
    "async": "^1",
    "debug": "^2",
    "@ebay/httpunch": "^1",
    "ip": "^1",
    "properties": "^1",
    "raptor-cache": "^1",
    "try-require": "^1"
  }
}
