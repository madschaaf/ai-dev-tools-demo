{
    "services": {
        "oauthAppTokenSvcV2": {
            "urn": "urn:REST:oauth",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "oauth.vip.ebay.com",
            "basepath": "/identity/v1/oauth2/token",
            "transport": "generic",
            "method": "POST",
            "https": "eBayRootCert",
            "forceRetry": true,
            "retry": 1,
            "socketTimeout": 1000,
            "headers": {
                "content-type": "application/x-www-form-urlencoded",
                "Accept": "application/json"
            },
            "rejectUnauthorized": false,
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        },
        "oauthDetailSvc": {
            "urn": "urn:REST:oauthclnt",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "oauthclnt.vip.ebay.com",
            "basepath": "/registry/v1",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "headers": {
                "Content-Type": "application/json",
                "Accept": "application/json"
            },
            "rejectUnauthorized": false,
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    },
    "oauth-ebay": {
        "oauth-enableDistributedCache": false,
        "jwt": "provider=IAM;type=JWT;managed=true"
    }
}
