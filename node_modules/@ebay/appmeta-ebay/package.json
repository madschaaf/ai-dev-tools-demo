  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "@ebay/environment-ebay": "^1",
    "@ebay/esams-consumer-ebay": "^4",
    "@ebay/logging-inc": "^4",
    "@ebay/service-client-ebay": "^4",
    "async": "^3",
    "debug": "^4",
    "raptor-async": "^1",
    "raptor-cache": "^2",
    "try-require": "^1",
    "xml2js": "^0"
  },
  "devDependencies": {
    "@ebay/module-config-inc": "^4",
    "@ebay/validate-internals-ebay": "^5",
    "dtslint": "^4.2.1",
    "express": "^4",
    "jshint": "^2",
    "mocha": "^8",
