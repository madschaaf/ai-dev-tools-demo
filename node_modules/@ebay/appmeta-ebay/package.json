{
  "author": "dsemenov, weima <weima@ebay.com>",
  "name": "@ebay/appmeta-ebay",
  "version": "4.3.0",
  "description": "app metadata for ebay",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "jshint ./lib ./test --config .jshintrc",
    "test:types": "dtslint types",
    "test:spec": "npm run clean && NODE_ENV=test DEPLOY_ENV=test mocha --timeout 10000 --require ./test/fixture/env.js test/*.js",
    "test": "npm run test:types && npm run test:spec",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test:spec",
    "clean": "rm -rf  .beans .cache"
  },
  "repository": {
    "type": "git",
    "url": "git@github.corp.ebay.com:nodejs/appmeta-ebay.git"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.corp.ebay.com/nodejs/appmeta-ebay",
  "bugs": {
