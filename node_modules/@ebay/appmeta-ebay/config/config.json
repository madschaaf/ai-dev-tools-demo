{
    "services": {
        "appmeta-rest-service": {
            "urn": "urn:REST:appmetasvc",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "appmetasvc.vip.ebay.com",
            "basepath": "/appmetasvc/v1/appMeta",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "headers": {
                "Content-Type": "application/json",
                "Accept": "application/json"
            },
            "instruments": ["error-handler", "retry", "config-bean", "cal"],
            "error-handler" : {
                "success-codes": [404]
            }
        },
        "oauth-detail-service": {
            "urn": "urn:REST:oauthclnt",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "oauthclnt.vip.ebay.com",
            "basepath": "/oauthclnt/core/v1",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "headers": {
                "Content-Type": "application/json",
                "Accept": "application/json"
            },
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }

    }
}
