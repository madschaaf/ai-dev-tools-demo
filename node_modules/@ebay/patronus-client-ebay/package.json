{
  "name": "@ebay/patronus-client-ebay",
  "version": "1.8.2",
  "description": "patronus client for Node.js",
  "main": "index.js",
  "types": "types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test:types": "tsd --show-diff",
    "test:spec": "rm -rf .beans .cache && NODE_ENV=test DEPLOY_ENV=test mocha --reporter spec --timeout 20s",
    "test": "npm run test:types && npm run test:spec",
    "lint": "eslint . test -f checkstyle -c .eslintrc",
    "lint:fix": "eslint --fix . test",
    "coverage": "NODE_ENV=test DEPLOY_ENV=test nyc --reporter=lcov --reporter=text-summary --dir gh-pages/coverage npm run test:spec"
  },
  "repository": {
    "type": "git",
    "url": "git@github.corp.ebay.com:nodejs/patronus-client-ebay.git"
  },
  "keywords": [
    "patronus client"
  ],
  "author": "mdathrika@ebay.com",
  "license": "ISC",
  "dependencies": {
    "@ebay/logging-inc": "^4",
    "@ebay/service-client-ebay": "^4",
    "@ebay/spellcaster-ebay": "^1.0.0",
    "@ebay/trust-fabric-ebay": "^1",
    "@jsdevtools/ono": "^7.1.3"
  },
  "devDependencies": {
    "@ebay/cal": "^4",
    "@ebay/monitor-inc": "^5.1.0",
    "@ebay/validate-internals-ebay": "^5",
    "@types/express": "^4.17.21",
    "eslint": "^5.0.0",
    "eslint-config-ebay": "^1.0.0",
    "eslint-plugin-mocha": "^5.0.0",
    "mocha": "^6",
    "nock": "^13.3.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.0",
    "tsd": "^0.31.1",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  }
}
