{
    "services": {
        "patronus-client": {
            "urn": "urn:REST:patronus",
            "protocol": "https:",
            "hostname": "patronus.vip.ebay.com",
            "basepath": "/v1/api",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "instruments": ["https", "error-handler", "retry", "config-bean", "cal", "trust-fabric"],
            "trust-fabric": {
                "addAuthorizationHeader": true,
                "force": true
            },
            "error-handler": {
                "success-codes": [404]
            }
        },

        "patronus-whitelist": {
            "urn": "urn:REST:spellcaster",
            "dnslookup": false,
            "requestCert": false,
            "rejectUnauthorized": false,
            "protocol": "https:",
            "hostname": "spellcaster.vip.ebay.com",
            "basepath": "/v1/api/client-apps/",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "https": "eBayRootCert",
            "instruments": ["https", "error-handler", "retry", "config-bean", "cal"]
        },

        "patronus-get-key": {
            "urn": "urn:REST:patronus",
            "dnslookup": false,
            "requestCert": false,
            "rejectUnauthorized": false,
            "protocol": "https:",
            "hostname": "patronus.vip.ebay.com",
            "basepath": "/v1/api/keys/",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "https": "eBayRootCert",
            "instruments": ["https", "error-handler", "retry", "config-bean", "cal"]
        },

        "patronus-channel-key": {
            "urn": "urn:REST:patronus",
            "dnslookup": false,
            "requestCert": false,
            "rejectUnauthorized": false,
            "protocol": "https:",
            "hostname": "patronus.vip.ebay.com",
            "basepath": "/v1/api/channels/",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "https": "eBayRootCert",
            "instruments": ["https", "error-handler", "retry", "config-bean", "cal"]
        }
    }
}