{
  "name": "@ebay/environment-ebay",
  "version": "1.3.0",
  "description": "Node environment selector for eBay",
  "main": "index.js",
  "author": "Sanjeev Koranga",
  "homepage": "https://github.corp.ebay.com/nodejs/environment-ebay.git",
  "repository": {
    "type": "git",
    "url": "https://github.corp.ebay.com/nodejs/environment-ebay.git"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "test:types": "tsd",
    "test:spec": "NODE_ENV=test DEPLOY_ENV=test mocha test/*.js",
    "test": "yarn test:types && yarn test:spec",
    "lint": "jshint ./lib --config .jshintrc",
    "coverage": "nyc --dir gh-pages/coverage yarn test:spec"
  },
  "keywords": [
    "environment",
    "nodejs"
  ],
  "license": "BSD",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tsd": "0.31.0",
    "jshint": "^2",
    "mocha": "^8",
    "nyc": "^15.1.0",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  }
}
