{
    "service-discovery-ebay": {
        "enabled": false
    },

    "services": {
        "authSvcDiscovery": {
            "protocol": "https:",
            "basepath": "/authzsvc/api/v1/servicediscovery/getServiceInstances",
            "hostname": "authzsvc-tls.vip.ebay.com",
            "transport": "generic",
            "socketTimeout": 5000,
            "retry": 3,
            "discovery": false,
            "rejectUnauthorized": false,
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    },
    
    "@ConfigBean": {
        "service-discovery-ebay": {
            "common": [
                "service-discovery-ebay:enabled"                
            ]
        }
    }
}
