{
    "security-ebay": {
        "request-host-validation": true,
        "domains": [],
        "eBayDomains": [],
        "native-domains": [],
        "enabled": true,
        "WhitelistPerAppname":{},
        "content-type-enabled": true,
        "sanitizeParameterKeys": false,
        "hsts": {
          "enabled": false,
          "max_age": "zero"
        },
        "qualyscan-filter": false,
        "sensitive-paramaters": [
          "Ccnumber",
          "cvvno",
          "ccyear",
          "cardNumber",
          "Cardnumber",
          "cvv2num",
          "CVVNum",
          "CardNum",
          "ccexpirationyear",
          "ccexpirationmonth",
          "cardnumber",
          "ExpDtMon",
          "ExpDtYr",
          "cardIdNumber",
          "username",
          "address1",
          "address2",
          "city",
          "zip",
          "rpass",
          "password",
          "pass",
          "id_token"
      ],
      "context": [
          "id_token"
      ],
      "xss-body-check-whitelist" : {}
    },

    "xss-security-ebay": {
        "xss.UrlCheck" : "false",
        "xss.requestParameterCheck" : "false",

        "xss.strictMode": "true",
        "xss.fullExemptions" : "",
        "xss.partialExemptions" :  ""
    },

    "@ConfigBean": {
        "security-ebay": {
            "common": [
                "security-ebay",
                "enabled"
            ]
        }
    },
    
    "@RaptorConfig": {
        "security-ebay": [{
            "target": "Global",
            "project": "framework",
            "config": "security-ebay",
            "version": "0.0.1",
            "keyPath": "security-ebay"
        },
        {
            "target": "Global",
            "project": "framework",
            "config": "blade-nodejs",
            "version": "0.0.1",
            "keyPath": "blade"
        }]
    }
}
