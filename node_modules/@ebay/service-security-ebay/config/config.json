{
    "service-security-ebay": {
        "usertokensessionmgmt": true
    },
    "identity-context-ebay": {
        "gateway-scopes": ["https://api.ebay.com/oauth/scope/@federated"],
        "header-name": "x-ebay-c-fgql-ictxt",
        "allowed-clock-skew-seconds": 120
    },
    "services": {
        "usertokensessionmgmt": {
            "protocol": "https:",
            "hostname": "usertokensessionmgmt.vip.ebay.com",
            "basepath": "/identity/v1/validation/token",
            "oauth": {
                "token": ["app"],
                "scope": "https://api.ebay.com/oauth/scope/core@application"
            },
            "headers": {
                "content-type": "application/json",
                "accept": "application/json"
            },
            "instruments": ["app-token", "error-handler", "retry", "config-bean", "cal"]
        }
    },
    "@ConfigBean": {
        "service-security-ebay": {
            "common": [
                "service-security-ebay"
            ]
        }
    }
}
