{
    "services": {
        "iafservice": {
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "iafsvc.vip.ebay.com",
            "basepath": "/ws/spf",
            "transport": "generic",
            "method": "POST",
            "socketTimeout": 1000,
            "force-retry": true,
            "retry": 2,
            "service": "IAFService",
            "namespaces": true,
            "wsdl": "path:./IAFService.wsdl",
            "instruments": ["soap", "error-handler", "retry", "config-bean", "cal"],
            "soap": {
                "guid": false
            },
            "https": {
                "keepAlive": true,
                "keepAliveMsecs": 20000
            }
        },
        "asac-iaf-service": {
            "rejectUnauthorized": false,
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "soacs.vip.ebay.com",
            "basepath": "/ws/spf",
            "transport": "generic",
            "method": "POST",
            "socketTimeout": 1000,
            "force-retry": true,
            "retry": 2,
            "service": "IAFService",
            "namespaces": true,
            "wsdl": "path:./IAFService.wsdl",
            "instruments": ["soap", "error-handler", "retry", "config-bean", "cal"]
        }
    }
}
