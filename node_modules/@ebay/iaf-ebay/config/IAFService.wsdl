			<xs:complexType abstract="true" name="ConditionAbstractType">
				<xs:annotation>
					<xs:documentation>Defines the Condition element abstract type. All types of Conditions inherit from this base type.</xs:documentation>
				</xs:annotation></xs:complexType>
			<xs:element name="AudienceRestriction" type="saml:AudienceRestrictionType">
				<xs:annotation>
					<xs:documentation>Specifies that the assertion is addressed to a particular audience.</xs:documentation>
				</xs:annotation></xs:element>
