                <xs:sequence>

					<xs:element name="identifier" type="iaf:Identifier">
                        <xs:annotation>
                        	<xs:documentation>Subject identifier.</xs:documentation>
                        </xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="identityDomain" type="xs:string">
					<xs:annotation>
						<xs:documentation>The identity provider domain where the subject belongs to.</xs:documentation>
					</xs:annotation></xs:attribute>
			</xs:complexType>


