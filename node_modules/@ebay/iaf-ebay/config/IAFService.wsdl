						<xs:sequence>
							<xs:element maxOccurs="unbounded" ref="saml:Audience">
								<xs:annotation>
									<xs:documentation>A URI reference that identifies an intended audience.</xs:documentation></xs:annotation></xs:element>
						</xs:sequence>
					</xs:extension>
				</xs:complexContent>
