					<xs:element name="token" type="xs:string">
						<xs:annotation>
							<xs:documentation>A collection of assertions expressed in a standard format and packaged securely according to domain policies</xs:documentation></xs:annotation></xs:element>
				</xs:sequence>
				<xs:attribute name="type" use="required">
