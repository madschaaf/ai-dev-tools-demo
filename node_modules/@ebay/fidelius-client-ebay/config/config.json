{
    "services": {
        "fidelius-read-objects": {
            "urn": "urn:REST:fidelius",
            "dnslookup": false,
            "requestCert": false,
            "rejectUnauthorized": false,
            "protocol": "https:",
            "hostname": "fidelius.vip.ebay.com",
            "basepath": "/v1/api/objects/",
            "transport": "generic",
            "method": "GET",
            "retry": 1,
            "socketTimeout": 1000,
            "https": "eBayRootCert",
            "instruments": ["https","error-handler", "retry", "config-bean", "cal"],
            "headers": {
                "Content-Type": "application/json"
            },
            "error-handler": {
                "success-codes": [
                    400, 403, 404
                ]
            }
        },
