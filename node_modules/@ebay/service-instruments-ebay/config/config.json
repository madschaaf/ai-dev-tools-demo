{
    "service-instruments-ebay": {
        "default-instruments": [
            "https",
            "aid",
            "ep",
            "tracking",
            "tracing",
            "trust-fabric",
            "outbound-context",
            "oauth",
            "circuit",
            "retry",
            "cal",
            "discovery",
            "metrics",
            "graphql",
            "traffic-mirror",
            "header-propagation",
            "sddz-proxy"
        ],
        "instruments-metadata": {
            "config": {
                "module": "@ebay/service-instruments-ebay/config",
                "priority": 5
            },
            "cal": {
                "module": "@ebay/service-instruments-ebay/cal",
                "priority": 10
            },
            "circuit": {
                "module": "@ebay/service-instruments-ebay/circuit",
                "priority": 15
            },
            "cache": {
                "module": "@ebay/service-instruments-ebay/cache",
                "priority": 20
            },
            "config-bean": {
                "module": "@ebay/service-instruments-ebay/noop",
                "priority": 20
            },
            "retry": {
                "module": "@ebay/service-instruments-ebay/retry",
                "priority": 30
            },
            "error-handler": {
                "module": "@ebay/service-instruments-ebay/circuit",
                "priority": 25
            },
            "soap": {
                "module": "@ebay/service-instruments-ebay/soap",
                "priority": 50
            },
            "app-token": {
                "module": "@ebay/service-instruments-ebay/app-token",
                "priority": 50
            },
            "oauth": {
                "module": "@ebay/service-instruments-ebay/oauth",
                "priority": 50
            },
            "discovery": {
                "module": "@ebay/service-instruments-ebay/discovery",
                "priority": 55
            },
            "outbound-context": {
                "module": "@ebay/service-instruments-ebay/outbound-context",
                "priority": 60
            },
            "aid": {
                "module": "@ebay/service-instruments-ebay/aid",
                "priority": 61
            },
            "trust-fabric": {
                "module": "@ebay/service-instruments-ebay/trust-fabric",
                "priority": 61
            },
            "traffic-mirror": {
                "module": "@ebay/service-instruments-ebay/traffic-mirror",
                "priority": 62
            },
            "identity-context": {
                "module": "@ebay/service-instruments-ebay/identity-context",
                "priority": 63
            },
            "header-propagation": {
                "module": "@ebay/service-instruments-ebay/header-propagation",
                "priority": 64
            },
            "tracing": {
                "module": "@ebay/service-instruments-ebay/tracing",
                "priority": 65
            },
            "sddz-proxy": {
                "module": "@ebay/service-instruments-ebay/sddz-proxy",
                "priority": 66
            },
            "tracking": {
                "module": "@ebay/service-instruments-ebay/tracking",
                "priority": 70
            },
            "ep": {
                "module": "@ebay/service-instruments-ebay/ep",
                "priority": 80
            },
            "https": {
                "module": "@ebay/service-instruments-ebay/https",
                "priority": 90
            },
            "cookies": {
                "module": "@ebay/service-instruments-ebay/cookies",
                "priority": 100
            },
            "graphql": {
                "module": "@ebay/service-instruments-ebay/graphql",
                "priority": 110
            },
            "compress": {
                "module": "@ebay/service-instruments-ebay/compress",
                "priority": 1000
            },
            "metrics": {
                "module": "@ebay/service-instruments-ebay/metrics",
                "priority": 1010
            },
            "stream-response": {
                "module": "@ebay/service-instruments-ebay/stream-response",
                "priority": 3000
            },
            "generic": {
                "module": "@ebay/service-instruments-ebay/transports/http-transport",
                "transport": true
            },
            "rest": {
                "module": "@ebay/service-instruments-ebay/transports/http-transport",
                "transport": true
            },
            "sifr": {
                "module": "@ebay/service-instruments-ebay/transports/sifr-transport",
                "transport": true
            },
            "xhr": {
                "module": "@ebay/service-instruments-ebay/transports/xhr-transport",
                "transport": true
            },
            "sse": {
                "module": "@ebay/service-instruments-ebay/transports/sse",
                "transport": true
            },
            "mje": {
                "module": "@ebay/service-instruments-ebay/transports/mje",
                "transport": true
            },
            "simple-chunk": {
                "module": "@ebay/service-instruments-ebay/transports/simple-chunk",
                "transport": true
            },
            "json-stream": {
                "module": "@ebay/service-instruments-ebay/transports/json-stream",
                "transport": true
            },
            "mock-transport": {
                "module": "@ebay/service-instruments-ebay/transports/mock-transport",
                "transport": true
            },
            "mock-multi-chunk": {
                "module": "@ebay/service-instruments-ebay/transports/mock-multi-chunk",
                "transport": true
            }
        },
        "prometheus": {
            "enabled": true,
            "histogramBuckets": [5, 10, 20, 40, 80, 160, 320, 640, 1280, 2560, 5120, 10240],
            "histogramBuckets_platform_cloud": [5, 10, 15, 20, 25, 30, 35, 40, 45, 50, 100, 200, 400],
            "uriEnabled": false
        }
    },
    "SvcVipMappingConfig": {
        "recognizedDomains": [
            ".stratus",
            ".vip"
        ],
        "patternListForMapping": [
            ".ebay.com",
            ".stratus.ebay.com"
        ],
        "vipMapping": {
            "raptordds.vip": "www.raptor-dds.stratus.ebay.com",
            "seskeysvc.vip": "seskeysvc.vip.ebay.com",
            "sessionkeysvc.vip": "seskeysvc.vip.ebay.com",
            "resreposvc.vip": "resreposvc.vip.ebay.com",
            "esams.vip": "esams.vip.ebay.com",
            "pes.vip": "pes.vip.ebay.com",
            "idauth.vip": "idauth.vip.ebay.com",
            "oauthclnt.vip": "oauthclnt.vip.ebay.com",
            "globalheader.vip": "www.gh.stratus.ebay.com",
            "iafservice.vip": "soaident.vip.ebay.com",
            "epsvc.vip": "www.epsvc.stratus.ebay.com",
            "authzsvc.vip": "authzsvc.vip.ebay.com"
        }
    },
    "sddz": {
        "proxyUrl": "http://proxy.vip.sddz.ebay.com:80",
        "proxyAllowedHosts": [],
        "proxyReadTimeout": 5000
    },

    "@ConfigBean": {
        "service-instruments-ebay": {
            "common": [
                "SvcVipMappingConfig",
                "sddz"
            ],
            "metrics": [
                "service-instruments-ebay:prometheus"
            ]
        }
    },
    "@RaptorConfig": {
        "service-instruments-ebay": [
            {
                "target": "Global",
                "project": "Raptor",
                "config": "SvcVipMappingConfig",
                "version": "1.0.0",
                "keyPath": "SvcVipMappingConfig"
            },
            {
                "target": "Global",
                "project": "sddz",
                "config": "sddz",
                "version": "1.0.0",
                "keyPath": "sddz:proxyAllowedHosts"
            }
        ]
    }
}
