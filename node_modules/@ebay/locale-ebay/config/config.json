{
    "locale": {     //locale namespace
        "EnabledSiteIdList": [0],
        "UserAgentMapping": [".*Yandex.*=RUru-RU"],
        "Enable_Geo_Expansion": true,
        "GBH_EXCLUSION_SITE_IDS": {
            "GBH_HEADER": "215|31"
        },
        "GBH_SUBDOMAIN_EXCLUSION_SITE_IDS": {
            "GBH_HEADER": "215|22|107|31|206|12|28|44|46|51|60|61|63|85|91|136|150|158|160|214|225|248|252"
        },
        "GeoCountries": ["US"]
    },
    "locale-ebay": {
        "plans": {
            "default": ["viaSiteInfo", "viaDefault", "geoByCountryCode"],
            "nongeo": ["viaUserAgent", "viaUrl", "viaBean", "viaUserLocale", "userByIp", "viaSiteInfo", "viaDefault", "geoByCountryCode"],
            "geo": ["viaUserAgent", "viaUrl", "viaBean", "viaCookie", "countryByIp", "viaSiteInfo", "viaDefault", "geoByCountryCode"],
            "geoseo": ["viaUserAgent", "viaUrl", "viaBean", "viaCookie", "countryByIp", "seoURL", "viaSiteInfo", "viaDefault", "geoByCountryCode"]
        },
        "accept-language": false,
        "subdomainEnabled": false
    },

    "@ConfigBean": {
        "locale-ebay": [
            "locale",
            "locale-ebay"
        ]
    },
    "@RaptorConfig": {
        "locale-ebay": [
            {
                "target": "Global",
                "project": "geo",
                "config": "GeoConfig",
                "version": "2.0.0",
                "keyPath": "locale"     //This is to map this raptorconfig with locale namespace
            }
        ]
    }
}
