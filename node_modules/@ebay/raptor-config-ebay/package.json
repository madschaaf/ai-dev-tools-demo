{
  "name": "@ebay/raptor-config-ebay",
  "version": "4.2.7",
  "description": "raptor config client to load configs from mongo based raptor config service",
  "main": "index.js",
  "scripts": {
    "lint": "jshint ./lib --config ./.jshintrc && jshint ./test --config ./.jshintrc",
    "test": "rm -rf .beans .cache && yarn test:spec && yarn test:cronus",
    "test:spec": "NODE_ENV=test DEPLOY_ENV=test mocha --require test/fixtures/utils --reporter spec --timeout 10000 --invert --grep \"force-update-test.js\" test/*.js",
    "test:cronus": "NODE_ENV=test DEPLOY_ENV=test mocha --require test/fixtures/utils --reporter spec --timeout 10000 --grep \"force-update-test.js\" test/*.js",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "author": "Sanjeev Koranga <skoranga@paypal.com>",
  "contributors": [
    "Dmytro Semenov <dsemenov@ebay.com>",
    "Chinnanadar Shenbagamoorthy, Vignesh <vics@ebay.com>"
  ],
  "homepage": "http://github.corp.ebay.com/nodejs/raptor-config-ebay",
  "repository": {
    "type": "git",
    "url": "http://github.corp.ebay.com/nodejs/raptor-config-ebay.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  },
  "keywords": [
    "ebay",
    "nodejs",
    "config"
  ],
  "license": "BSD",
  "readmeFilename": "README.md",
  "dependencies": {
    "@ebay/config-bean-ebay": "^0",
    "@ebay/httpunch": "^1",
    "@ebay/module-config-inc": "^4",
    "@ebay/service-client-ebay": "^4",
    "fs-extra": "^7.0.1",
    "@jsdevtools/ono": "^7.1.3",
    "async": "^1",
    "bit-set": "^1",
    "caller": "^1",
    "debug": "^2",
    "tar-fs": "^1.16.3",
    "underscore": "^1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@ebay/monitor-inc": "^5",
    "@ebay/validate-internals-ebay": "^5",
    "chai": "^6.0.1",
    "express": "^4",
    "jshint": "^2",
    "mocha": "^3",
    "nock": "^13.3.2",
    "nyc": "^15.1.0",
    "sinon": "^21.0.0",
    "supertest": "^3"
  }
}
