{
    "refreshconfig": {
        "domain": "E|qa.ebay.com"
    },
    "services": {
        "configpublisher": {
            "rejectUnauthorized": false,
            "protocol": "https:",
            "hostname": "configpublisher.vip.qa.ebay.com",
            "port": "443",
            "basepath": "/globalconfig/config",
            "transport": "generic",
            "socketTimeout": 15000,
            "method": "GET",
            "retry" : 1,
            "error-handler" : {
                "success-codes": [404, 400]
            },
            "instruments": [ "https", "error-handler", "retry", "config-bean", "cal"]
        }
    },
    "raptor-config-ebay": {
        "globalconfig-root": ".cache/globalconfig",
        "swift-connect-timeout": 1000,
        "swift-socket-timeout": 5000
    }
}
