{
    "services": {
        "protego-client-ebay": {
            "protocol": "https:",
            "hostname": "protegopub.vip.ebay.com",
            "basepath": "/publish/v1",
            "port": "443",
            "socketTimeout": 10000,
            "tracing": false,
            "metrics": false,
            "discovery": false,
            "instruments": ["https", "trust-fabric", "error-handler", "retry", "config-bean", "cal"],
            "trust-fabric": {
                "addAuthorizationHeader": true,
                "force": true
            },
            "headers": {
                "accept": "text/plain"
            }
        }
    }
}