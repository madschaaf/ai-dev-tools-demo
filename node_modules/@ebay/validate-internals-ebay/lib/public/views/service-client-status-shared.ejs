<Component name="Service Client Status" alias="serviceclientstatus" status="created">

<% for(var i=0; i<components.length; i++){
    var comp = components[i] %>

    <Properties>
    <Property name="ConfigCategory Id"><![CDATA[<%=comp.name%>]]></Property>
    <Property name="MarkdownReason">
    <![CDATA[ <%=comp.markdownReason%> ]]>
    </Property>
    <Property name="MarkdownStartTime">
    <![CDATA[ <%=comp.markdownStartTime%> ]]>
    </Property>
    <Property name="NioSvcClient">
    <![CDATA[ true ]]>
    </Property>
    <Property name="Service Name"><![CDATA[<%=comp.name%>]]></Property>
    <Property name="Status">
    <![CDATA[ <%=comp.status%> ]]>
    </Property>
    <Property name="Effective Endpoint">
        <![CDATA[ <%=comp.endpoint%> ]]>
    </Property>
    <Property name="Original Endpoint">
        <![CDATA[ <%=comp.origendpoint%> ]]>
    </Property>
    <Property name="TLS Version">
        <![CDATA[ <%=comp.tlsVersion%> ]]>
    </Property>
    <Property name="Service Id">
        <![CDATA[ <%=comp.urn%> ]]>
    </Property>
    </Properties>

<% } %>

</Component>
