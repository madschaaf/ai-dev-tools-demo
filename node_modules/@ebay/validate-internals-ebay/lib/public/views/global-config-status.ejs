<% if(!forceXml){ %> <?xml-stylesheet type="text/xsl" href="resources/xsl/GlobalConfigStatus.xsl"?><% } else { %><?xml version="1.0" encoding="UTF-8"?><% } %>
<ValidateInternals xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" envoyEnabled="<%=envoyEnabled%>" containerPdlcEnabled="<%=containerPdlcEnabled%>" host="<%= host %>/<%= ipAddress %>"
                   client-ip="<%= clientIpAddress %>" uiTheme="../css/raptor_page.css"
                   xsi:noNamespaceSchemaLocation="resources/xsd/ValidateInternals.xsd" more-info="HeaderPH">

  <%- include('header'); %>

  <Component name="<%= component.name %>" alias="<%= component.alias %>" status="created">

    <Properties>
      <% var props = component.properties
      for(var i = 0; i < props.length; i++){
        var prop = props[i]
      if (prop.name === "Config Manifest") {
      %>
        <Property <% if (prop.status !== "OK") { %>warningLevel="0"<% } %>
                  name="<%= prop.name %>" status="<%= prop.status %>"><![CDATA[<%- prop.value %>]]></Property>
      <% } else { %>
        <Property <% if (prop.status !== "OK") {
                    if (prop.required == true) { %>warningLevel="0"
                    <% } else { %>warningLevel="2"<% } }%>
                  name="<%= prop.name %>" status="<%= prop.status %>"
                  version="<%= prop.version %>" config="<%= prop.config %>" target="<%= prop.target %>"><![CDATA[<%- prop.value %>]]></Property>
      <% } %>
      <% } %>
    </Properties>
  </Component>

</ValidateInternals>
