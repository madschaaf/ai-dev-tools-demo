{
    "device-detection-ebay": {
        "cache": {
            "enabled": true,
            "max": 2000, //Cache size
            "maxAge": 1800000 //30 min TTL
        },
        "forceDdsVersion": "NOT_SET"
    },
    "services": {
        "device-detection-ebay": {
            "urn": "urn:REST:raptordds",
            "protocol": "https:",
            "dnslookup": false,
            "hostname": "raptordds.vip.ebay.com",
            "basepath": "/raptor-dds/DDSResource/v2",
            "transport": "generic",
            "socketTimeout": 2000,
            "retry": 1,
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        },
        "device-detection-ebay-v3": {
            "urn": "urn:REST:raptordds",
            "protocol": "https:",
            "dnslookup": false,
            "hostname": "raptordds.vip.ebay.com",
            "basepath": "/raptor-dds/DDSResource/v3",
            "transport": "generic",
            "socketTimeout": 2000,
            "retry": 1,
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    },
    "@ConfigBean": {
        "device-detection-ebay": {
            "common": [
                "device-detection-ebay"
            ]
        }
    },
    "@RaptorConfig": {
        "device-detection-ebay": [{
            "target": "Global",
            "project": "nodedds",
            "config": "nodedds",
            "version": "1.0.0",
            "keyPath": "device-detection-ebay"
        }]
    }
}
