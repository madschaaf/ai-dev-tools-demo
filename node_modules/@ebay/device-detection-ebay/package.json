{
  "name": "@ebay/device-detection-ebay",
  "version": "4.3.5",
  "description": "A server side utility (written as express middleware) to detect information of the client device based on the userAgent header of incoming request",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "jshint ./lib --config ./.jshintrc && jshint ./test --config ./.jshintrc",
    "test": "yarn test:spec && yarn test:types",
    "test:spec": "yarn clean & NODE_ENV=test DEPLOY_ENV=test mocha --reporter spec --timeout 10s test/*.js",
    "test:types": "tsd",
    "coverage": "nyc yarn test:spec",
    "clean": "rm -rf  ./.beans ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.corp.ebay.com/nodejs/device-detection-ebay.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  },
  "keywords": [
    "device",
    "detection",
    "useragent"
  ],
  "author": "Chothendavida, Subeesh <suchothendav@paypal.com>",
  "license": "BSD-2-Clause",
  "dependencies": {
    "@ebay/app-context-ebay": "^1.0.10",
    "@ebay/client-hints-ebay": "^1",
    "@ebay/config-bean-ebay": "^0",
    "@ebay/logging-inc": "^4.0.5",
    "@ebay/module-config-inc": "^4",
    "@ebay/raptor-config-ebay": "^4",
    "@ebay/service-client-ebay": "^4",
    "debug": "^4",
    "express-useragent": "^1",
    "lru-backup-cache": "^1"
  },
  "devDependencies": {
    "@ebay/monitor-inc": "^5.1.0",
    "@ebay/validate-internals-ebay": "^5",
    "@types/express": "^4.17.8",
    "async": "^3",
    "extend": "^3",
    "jshint": "^2",
    "mocha": "^8",
    "nock": "^13",
    "nyc": "^15",
    "tsd": "^0.30.3",
    "typescript": "^4.0.2"
  }
}
