{
    "name": "@ebay/client-hints-ebay",
    "version": "1.0.0",
    "description": "Client Hints Config",
    "main": "index.js",
    "scripts": {
        "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
        "lint": "eslint -c .eslintrc *.js",
        "lint-fix": "eslint -c .eslintrc --fix *.js",
        "test": "rm -rf .beans .cache && NODE_ENV=test DEPLOY_ENV=test mocha test/*.js --timeout 10s"
    },
    "repository": {
      "type": "git",
      "url": "https://github.corp.ebay.com/nodejs/client-hints-ebay.git"
    },
    "publishConfig": {
      "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
    },
    "keywords": [
      "device",
      "detection",
      "useragent"
    ],
    "author": "mdathrika@ebay.com",
    "license": "BSD-2-Clause",
    "dependencies": {
        "@ebay/module-config-inc": "^4",
        "@ebay/service-client-ebay": "^4",
        "@ebay/config-bean-ebay": "^0",
        "@ebay/logging-inc": "^4.0.5",
        "@ebay/app-context-ebay": "^1.0.10"
    },
    "devDependencies": {
        "eslint": "^7.8.1",
        "eslint-config-ebay": "^1.1.2",
        "eslint-plugin-mocha": "^8.0.0",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "nock": "^13",
        "@ebay/validate-internals-ebay": "^5.3.2",
        "@ebay/monitor-inc": "^5.1.0"
    }
  }
  