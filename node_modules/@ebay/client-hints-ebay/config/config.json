{
    "client-hints-ebay": {
        "clientHint":{
            "enable":false,
            "requestHeaders":["sec-ch-ua-model","sec-ch-ua-platform-version","sec-ch-ua-full-version"],
            "chainHeaders":["sec-ch-ua-model","sec-ch-ua-platform-version","sec-ch-ua-full-version"],
            "acceptCHs":["sec-ch-ua-model","sec-ch-ua-platform-version","sec-ch-ua-full-version"]
        },
        "configRefreshIntervel": 3600000
    },
    "services": {
        "client-hints-config": {
            "urn": "urn:REST:raptordds",
            "protocol": "https:",
            "dnslookup": false,
            "hostname": "raptordds.vip.ebay.com",
            "basepath": "/raptor-dds/DDSResource/v2",
            "transport": "generic",
            "socketTimeout": 2000,
            "retry": 1,
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    },
    "@ConfigBean": {
        "client-hints-ebay": {
            "common": [
                "client-hints-ebay"
            ]
        }
    }
}
