    "@ebay/instance-detect-ebay": "^1",
    "@ebay/module-config-inc": "^4",
    "@ebay/utils-ebay": "^1",
    "prom-client": "^15.0.0-1"
  },
  "devDependencies": {
    "@ebay/brogan-ebay": "^4",
    "@ebay/cal": "^4",
    "@ebay/commons-inc": "^4",
    "eslint": "^8.18.0",
    "eslint-config-ebay": "^1.1.5",
    "eslint-plugin-mocha": "^10.0.5",
    "express": "^4.16.4",
    "kraken-js": "^2.2.0",
    "meta-router": "^3.0.2",
    "mocha": "^8",
    "nock": "^9",
    "nyc": "^15.1.0",
    "shelljs": "^0.3",
    "supertest": "^3.4.2",
    "tsd": "^0.17.0",
    "typescript": "^4.9.4"
  },
  "license": "ISC",
