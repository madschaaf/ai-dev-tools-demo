{
  "name": "@ebay/prometheus-ebay",
  "version": "3.5.7",
  "description": "Prometheus module for exposing Node.js metrics",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test:types": "tsd",
    "test:spec": "NODE_ENV=test DEPLOY_ENV=test mocha test/*.js --require test/fixtures/env.js --timeout 15s",
    "test": "yarn test:types && yarn test:spec",
    "lint": "eslint --quiet -c .eslintrc .",
    "lint-fix": "eslint --quiet -c .eslintrc --fix .",
    "coverage": "NODE_ENV=test DEPLOY_ENV=test nyc --dir gh-pages/coverage npm run test:spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.corp.ebay.com/nodejs/prometheus-ebay.git"
  },
  "keywords": [
    "Prometheus",
