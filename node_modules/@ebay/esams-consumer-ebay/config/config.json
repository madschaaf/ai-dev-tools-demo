{
    "services": {
        "esamsConsumerSvc": {
            "urn": "urn:SOA:EsamsService",
            "rejectUnauthorized": false,
            "wsdl": "path:./esamsService.wsdl",
            "protocol": "https:",
            "hostname": "esams-secure.vip.ebay.com",
            "basepath": "/ws/spf",
            "socketTimeout": 1000,
            "namespaces": true,
            "method": "POST",
            "retry": 1,
            "force-retry": true,
            "xff": false,
            "headers": {
                "X-EBAY-SOA-SERVICE-VERSION": "1.0.0"
            },
            "instruments": ["soap", "error-handler", "retry", "config-bean", "cache", "cal"]
        }
    },

    "name-service": {
        "clockDiscrepancy": 0,
        "legacyEsams": false
    },

    "@ConfigBean": {
        "esams-consumer-ebay": {
            "name-service": [
                "name-service"
            ]
        }
    }
}
