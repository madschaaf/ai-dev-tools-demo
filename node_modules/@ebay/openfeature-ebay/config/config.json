{
    "services": {
        "featureflag": {
            "urn": "urn:REST:epsvc",
            "socketTimeout": 2000,
            "protocol": "https:",
            "hostname": "epsvc.vip.ebay.com",
            "port": "443",
            "basepath": "/epexesvc/ExperimentationExecutionService/v1/feature_flags/evaluated/internal_search",
            "transport": "generic",
            "method": "POST",
            "force-retry": true,
            "https": {
                "keepAlive": true,
                "keepAliveMsecs": 20000
            },
            "headers": {
                "content-type": "application/json",
                "accept": "application/json"
            },
            "app-token": {
                "scope": ["https://api.ebay.com/oauth/scope/core@application"]
            },
            "instruments": [
                "error-handler",
                "retry",
                "config-bean",
                "cal",
                "app-token"
            ]
        }
    },
    "prometheus": {
        "enabled": true,
        "interceptRequest": false,
        "tlsClientInfo": true,
        "histogramBuckets": [5, 10, 20, 40, 80, 160, 320, 640, 1280, 2560, 5120, 10240]
    },
    "openfeature": {
        "enabled": true,
        "localStorageEnabled": true,
        "errorTracing": true
    },
    "@ConfigBean": {
        "openfeature-ebay": {
            "common": [
                "openfeature"
            ]
        }
    }
}
