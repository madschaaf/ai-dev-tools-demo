{
  "name": "@ebay/openfeature-ebay",
  "version": "2.11.4",
  "description": "Open Feature for Node JS feature flagging and Experimentation",
  "main": "./dist/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf ./dist && rm -rf .beans && rm -rf .cache",
    "prepublishOnly": "npm run build",
    "format": "prettier \"**/*.{ts,js,json,md}\" --tab-width 4 --write",
    "test": "NODE_ENV=test mocha ./test/*.ts -r ts-node/register --exit --timeout 30s",
    "lint": "jshint ./src/openfeature-lib --config ./.jshintrc && jshint ./test --config ./.jshintrc",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test"
  },
  "author": "Leon Ieong <pieong@ebay.com>",
  "license": "ISC",
  "devDependencies": {
    "@ebay/locale-ebay": "^4.6.0",
    "@ebay/mock-app-ebay": "^4.0.0",
    "@ebay/tracking-ebay": "^4.14.3",
    "@ebay/validate-internals-ebay": "^5.5.2",
    "@types/mocha": "^9.1.1",
    "eslint": "^8.20.0",
    "eslint-config-ebay": "^1.1.5",
    "eslint-plugin-mocha": "^10.0.5",
    "jquery": "^3.6.0",
    "jsdom": "^19.0.0",
    "jshint": "^2.13.6",
    "mocha": "^10.0.0",
    "nock": "^13.0.4",
    "node-mocks-http": "^1.12.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sinon": "^15.0.1",
    "supertest": "^4",
    "ts-node": "^10.9.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.ebay.com/artifactory/api/npm/npm-nodejs/"
  },
  "dependencies": {
    "@ebay/logging-inc": "^4.1.2",
    "@ebay/opentelemetry-ebay": "^2.4.11",
    "@ebay/service-client-ebay": "^4",
    "@openfeature/js-sdk": "^1",
    "tslib": "^1.14.1"
  },
  "files": [
    "dist",
    "src",
    "config"
  ],
  "types": "dist/index.d.ts"
}
