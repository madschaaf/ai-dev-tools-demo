{
    "geoservice": {
        "cache": {
            "enabled": true,
            "max": 3000,
            "maxAge": 3600000
        }
    },
    "services": {
        "geoservice": {
            "urn": "urn:REST:geocfgsvc",
            "dnslookup": false,
            "protocol": "https:",
            "hostname": "www.geocfgsvc.stratus.ebay.com",
            "basepath": "/developer/geocfgsvc/v3",
            "socketTimeout": 2000,
            "transport": "generic",
            "method": "GET",
            "instruments": ["error-handler", "retry", "config-bean", "cal"]
        }
    }

}
